select 
DM.merchant_id,
DM.merchant_code,
dm.merchant_account_state,
dm.merchant_account_substate,
dm.legal_type_name,
dm.aml_risk_level,
dm.merchant_sign_up_at::date,
pb.tax_id
FROM global_analyst_prod.mart_growth.verification_merchant_verification_status DM 
left join SUMUP_DWH_PROD.SRC_PAYMENT.BUSINESS_DETAILS pb on pb.merchant_id = dm.merchant_id
where 1=1
and DM.merchant_code in ('MCNDNYE2')
